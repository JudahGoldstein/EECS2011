package model;

import tests.Node;

public class ListUtilities<T> {

	public Node<String> task(Node<String> n1, int n) {
		if(n==0) {
			return n1;
		}
		Node<String> current = n1;
		while(current.getNext()!=null) {
			current = current.getNext();
		}
		current.setNext(n1);
		current = n1;
		for(int i = 0; i<n;i++) {
			current = current.getNext();
		}
		n1=current.getNext();
		current.setNext(null);
		return n1;
	}

}
